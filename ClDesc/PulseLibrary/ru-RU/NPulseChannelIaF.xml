			<Type>| ptPubParameter |</Type>
			<DataSelectionType>0</DataSelectionType>
			<ValueList Type="std::vector" Size="2" elemType="std::string">
				<elem Type="std::string"></elem>
				<elem Type="std::string"></elem>
			</ValueList>
			<PropertyType>257</PropertyType>
